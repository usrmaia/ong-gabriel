{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma",
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "conventionalCommits.scopes": [
    "env",
    "proj",
    "git",
    "prettier",
    "logs",
    "prisma",
    "zod",
    "api",
    "tests",
    "tailwind",
    "auth",
    "permissions",
    "types",
    "form",
    "role",
    "attendance",
    "analytics"
  ]
}
